<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yd05.mprj.delicious.service.DeliciousMapper">

	<select id="selectDel" parameterType="com.yd05.mprj.delicious.service.DeliciousVO">
		SELECT * FROM DELICIOUS WHERE CONTENTID = #{contentid}
	</select>
	
	<insert id="insertDel" parameterType="com.yd05.mprj.delicious.service.DeliciousVO">
		INSERT INTO DELICIOUS SELECT #{contentid} #{addr1} #{first FROM DUAL A WHERE NOT EXISTS (SELECT 1 FROM DELICIOUS WHERE CONTENTID = #{contentid})
	</insert>
</mapper>